{
    "version": "1.0.0",
    "ignoreStackList": {
        "Level1": [
            "libc.so",
            "libclang_rt.asan-arm-android.so",
            "libclang_rt.asan-aarch64-android.so",
            "libc_fdleak_debug.so",
            "unknown",
            "watchdog",
            "kthread"
        ],
        "Level2": [
            "libart.so",
            "__switch_to",
            "dump_backtrace",
            "show_stack",
            "dump_stack",
            "java.lang.Object",
            "java.lang.Thread",
            "java.lang.reflect.Method.invoke",
            "com.android.internal.os.ZygoteInit.main",
            "com.android.internal.os.RuntimeInit"
        ],
        "Level3": [
            "android.os.BinderProxy",
            "panic",
            "android.os.SystemClock.sleep"
        ]
    },
    "file1":{
        "dir":"/data/system/dropbox/",
        "pattern":".*?app_anr@[0-9]{13}\\.txt(\\.gz)?",
        "featureid":690100100,
        "command":[
            {"name":"BasicParam", "source":"Process: (.*)", "s_process":"Process: (.*)"},
            {"name":"BasicParam", "source":"PID: (.*)", "s_pid":"PID: (.*)"},
            {"name":"BasicParam", "source":"Flags: (.*)", "s_flag":"Flags: (.*)"},
            {"name":"BasicParam", "source":"Package: (.*)", "s_package":"Package: (.*)", "s_appversion":"^Package: [^\\(]*\\((.*)\\)$"},
            {"name":"BasicParam", "source":"APPVMTYPE: (.*)", "s_appvmtype":"APPVMTYPE: (.*)"},
            {"name":"BasicParam", "source":"SYSVMTYPE: (.*)", "s_sysvmtype":"SYSVMTYPE: (.*)"},
            {"name":"BasicParam", "source":"Foreground: (.*)", "s_foreground":"Foreground: (.*)"},
            {"name":"BasicParam", "source":"Activity: (.*)", "s_activity":"Activity: (.*)"},
            {"name":"BasicParam", "source":"Subject: (.*)", "s_subject":"Subject: (.*)", "s_block_reason":"(?<=Subject: ).*?(?=\\(.*)"},
            {"name":"BasicParam", "source":"Lifetime: (.*)", "s_lifetime":"Lifetime: (.*)"},
            {"name":"BasicParam", "source":"Build: (.*)", "s_build":"Build: (.*)"},
            {"name":"BasicParam", "source":"\\d+% \\d+/([^:]+):.*", "s_topcpu":"\\d+% \\d+/([^:]+):.*"},
            {"name":"BasicParam", "source":"(\\d[\\.0-9]+)% TOTAL:", "s_totalcpu":"(\\d[\\.0-9]+)% TOTAL:", "s_iowait":".* TOTAL: .* (\\d[\\.0-9]+)% iowait .*"},
            {"name":"MatchCmdline", "source":"Cmd line: &@&s_process", "s_block_cmdline":"Cmd line: &@&s_process"},
            {"name":"MainCallTrace", "depend":"MatchCmdline", "source":"\"main\" prio=", "begin":"^\\s+at (.*)\\(.*", "end":"^\\s+at (.*)\\(.*", "paramset":["s_f1", "s_f2","s_f3"]}
        ]
    },
    "file2":{
        "dir":"/data/system/dropbox/",
        "pattern":".*?app_crash@[0-9]{13}\\.txt(\\.gz)?",
        "featureid":690100101,
