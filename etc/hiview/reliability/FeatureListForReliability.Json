    "file4":{
        "dir":"/data/system/dropbox/",
        "pattern":".*?(system_server_crash|SYSTEM_TOMBSTONE)@[0-9]{13}\\.txt(\\.gz)?",
        "featureid":690100000,
        "command":[
            {"name":"BasicParam", "source":"(?<=Process:\\s)(.*)", "s_process":"(?<=Process:\\s)(.*)"},
            {"name":"BasicParam", "source":"(?<=>>>\\s)(.*)+(?=<<<)", "s_process":"(?<=>>>\\s)(.*)+(?=<<<)"},
            {"name":"BasicParam", "source":"SYSVMTYPE: (.*)", "s_sysvmtype":"SYSVMTYPE: (.*)"},
            {"name":"BasicParam", "source":"signal ([0-9]+).*","s_sigmsg":"signal ([0-9]+).*"},
            {"name":"MainCallTrace", "source":"^\\s+at (.*)\\(.*", "begin":"^\\s+at (.*)\\(.*", "end":"^\\s+at (.*)\\(.*", "paramset":["s_f1", "s_f2","s_f3"]},
            {"name":"BasicParam", "source":"(?<=server\\.)[a-zA-Z]+(?=\\.)", "s_module":"(?<=server\\.)[a-zA-Z]+(?=\\.)"}
        ]
    },
    "file5":{
        "dir":"/data/system/dropbox/",
        "pattern":".*?system_server_watchdog@@[0-9]{13}\\.txt(\\.gz)?",
        "featureid":690100001,
        "command":[
            {"name":"BasicParam", "source":"Process: (.*)", "s_process":"Process: (.*)"},
            {"name":"BasicParam", "source":"Subject: Blocked in monitor(.*)", "s_subject":"Subject: Blocked in monitor(.*)", "s_block_reason":"(?<=Blocked in monitor )(.*)(?=on foreground thread \\()", "s_thread":"(?<=\\()(.*?)(?=\\))"},
            {"name":"BasicParam", "source":"Subject: Blocked in handler on(.*)", "s_subject":"Subject: Blocked in handler on(.*)", "s_block_reason":"(?<=Blocked in handler on )(.*)(?=\\()", "s_thread":"(?<=\\()(.*?)(?=\\))"},
            {"name":"BasicParam", "source":"Build: (.*)", "s_build":"Build: (.*)"},
            {"name":"BasicParam", "source":"SYSVMTYPE: (.*)", "s_sysvmtype":"SYSVMTYPE: (.*)"},
            {"name":"BasicParam", "source":"\\d+% \\d+/([^:]+):.*", "s_topcpu":"\\d+% \\d+/([^:]+):.*"},
            {"name":"BasicParam", "source":"(\\d[\\.0-9]+)% TOTAL:", "s_totalcpu":"(\\d[\\.0-9]+)% TOTAL:", "s_iowait":".* TOTAL: .* (\\d[\\.0-9]+)% iowait .*"},
            {"name":"BasicParam", "source":"----- pid (.*) at", "s_pid":"----- pid (.*) at"},
            {"name":"MatchCmdline", "source":"Cmd line: &@&s_process", "s_block_cmdline":"Cmd line: &@&s_process"},
            {"name":"MainCallTrace", "depend":"MatchCmdline", "source":"- waiting to lock <", "s_waiting_to_lock":"(?<=- waiting to lock <)(.*)(?=>)", "s_held_tid":"(?<=held by thread )(.*)", "s_f1":"^.*?- waiting to lock.*?$"},
            {"name":"BlockTrace", "depend":"MainCallTrace", "source":"prio=\\d tid=&@&s_held_tid ", "s_binder_name":"(?<=\")(.*)(?=\")"},
            {"name":"BlockStack", "depend":"BlockTrace", "source":"prio=\\d tid=&@&s_held_tid ", "begin":"^\\s+at (.*)\\(.*", "end":"- locked <&@&s_waiting_to_lock>", "paramset":["s_f2"]},
            {"name":"BinderStack", "depend":"BlockStack", "source":"binder transactions", "s_bind":""},
            {"name":"BinderThread", "depend":"BinderStack", "source":"(?=&@&s_binder_name\\))", "s_f3":"^.*?&@&s_binder_name.*?$"}
        ]
    },
